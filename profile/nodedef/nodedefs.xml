<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller -->
    <nodeDef id="WeatherFlow" nodeType="139" nls="ctl">
        <editors />
        <sts>
		<st id="ST" editor="bool" />
		<st id="GV4" editor="I_SECONDS" />
	</sts>
        <cmds>
           <sends>
             <cmd id="DON" />
             <cmd id="DOF" />
           </sends>
            <accepts>
              <cmd id="DISCOVER" />
              <cmd id="REMOVE_NOTICES_ALL" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>

